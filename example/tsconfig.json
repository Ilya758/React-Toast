{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "types": ["cypress"],
    "jsx": "react",
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "skipLibCheck": true
  },
  "extends": "../tsconfig.json",
  "include": ["../src"]
}
